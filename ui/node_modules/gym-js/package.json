{
  "name": "gym-js",
  "version": "0.2.0",
  "description": "A JavaScript Environment for Reinforcement Learning.",
  "main": "dist/env.js",
  "types": "dist/env.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "demo": "webpack --watch & live-server --port=8085",
    "docs": "typedoc ./src"
  },
  "keywords": [
    "reinforcement learning"
  ],
  "author": "Thomas Tumiel",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tom2718/gym.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "live-server": "^1.2.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.6",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@tensorflow/tfjs": "^1.2.1",
    "phaser": "^3.18.1"
  }
}
